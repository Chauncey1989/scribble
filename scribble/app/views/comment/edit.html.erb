<h2>Edit Post</h2>

<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :album %>
  <%= f.text_field :album %>

  <%= f.label :preview_url %>
  <%= f.text_field :Preview_url %>

  <%= f.label :artist_id %>
  <%= f.text_field :artist_id %>

  <%= f.submit %>

<% end %>
